{
  "name": "gestola-core",
  "keywords": [
    "theia-extension"
  ],
  "version": "0.0.0",
  "files": [
    "lib",
    "src"
  ],
  "dependencies": {
    "@theia/core": "latest",
    "@theia/cli": "latest"
  },
  "devDependencies": {
    "rimraf": "latest",
    "typescript": "~4.5.5"
  },
  "scripts": {
    "rebuild": "yarn run clean && yarn run build && yarn localize",
    "clean": "rimraf lib",
    "build": "tsc",
    "watch": "tsc -w",
    "localize": "cp -r ./src/backend/i18n ./lib/backend/i18n"
  },
  "theiaExtensions": [
    {
      "backend": "lib/backend/backend-module",
      "frontend": "lib/frontend/frontend-module"
    }
  ]
  
}